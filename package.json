{
  "name": "discord_100kc",
  "version": "1.0.1",
  "description": "Discord 100 Kill Challenge tracking bot. Users upload images of their times and get placed on a leaderboard",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "debug": "tsc -p tsconfig.json",
    "build": "tsc -p tsbuild.json",
    "compileBin": "7z a .bin/binaries.zip src/index.js res package.json -mx9",
    "compileRelease": "7z a .release/release.zip release -mx9",
    "compileAll": "npm run compileBin && npm run compileRelease",
    "update": "cd release && npm run update"
  },
  "keywords": [
    "100",
    "Kill",
    "Challenge",
    "CSGO",
    "Discord",
    "bot",
    "tracking",
    "Optical",
    "Character",
    "Recognition",
    "OCR"
  ],
  "author": "B1ANK3",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.21.1",
    "discord.js": "^12.4.1",
    "minimist": "^1.2.5",
    "sharp": "^0.26.3",
    "tesseract.js": "^2.1.4"
  },
  "engines": {
    "node": "12.x",
    "npm": "6.x"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "typescript": "^4.0.5"
  },
  "readme": "README.md"
}
